{
    "name": "javajanteam/ywt",
    "description": "YesWeTicket Base client",
    "keywords": [
        "yesweticket"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "./packages/tincket/client",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "./packages/tincket/laravel-services",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:yesweticket/packages/tincket-client.git",
            "options": {
                "ssh2": {
                    "pubkey_file": "~/.ssh/id_rsa_tincket.pub",
                    "privkey_file": "~/.ssh/id_rsa_tincket"
                }
            }
        },
        {
            "type": "vcs",
            "url": "git@gitlab.com:yesweticket/packages/tincket-laravel-services.git",
            "options": {
                "ssh2": {
                    "pubkey_file": "~/.ssh/id_rsa_tincket.pub",
                    "privkey_file": "~/.ssh/id_rsa_tincket"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "fideloper/proxy": "~3.3",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "mcamara/laravel-localization": "1.3.19",
        "tincket/client": "^1.7.0",
        "tincket/laravel-services": "^1.7.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}